{
  "name": "aci-deployment-tracker",
  "version": "1.0.0",
  "type": "module",
  "description": "Advanced ACI Deployment Tracker for Multi-Site Multi-Fabric Environments",
  "main": "electron/main.js",
  "homepage": "./",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron-dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron-build": "npm run build && electron-builder",
    "dist": "npm run build && electron-builder --publish=never",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "aci",
    "cisco",
    "deployment",
    "tracker",
    "multi-site",
    "fabric"
  ],
  "author": "Essential Energy",
  "license": "ISC",
  "devDependencies": {
    "@flydotio/dockerfile": "^0.7.10",
    "@types/electron": "^1.4.38",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "electron": "^37.2.5",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.6",
    "typescript": "^5.9.2",
    "vite": "^7.0.6",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "lucide-react": "^0.535.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^3.4.17"
  }
